{
  "name": "firebase-authentication-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng -o serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "cypress run",
    "test:github": "npm run clean:reports:github && cypress run",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "clean:reports:github": "rm -rf mochawesome-report/",
    "generate:html:report": "npm run merge:reports && npm run create:html:report",
    "merge:reports": "mochawesome-merge mochawesome-report/*.json > index.json",
    "create:html:report": "marge index.json --reportDir cypress-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "firebase": "^9.4.0",
    "rxfire": "^6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.0",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "cypress": "latest",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "typescript": "~4.3.5"
  }
}
